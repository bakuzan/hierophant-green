{
  "name": "hierophant-green",
  "private": true,
  "description": "Seasonal Anime Statistics",
  "version": "0.11.1",
  "author": "bakuzan",
  "dependencies": {
    "gatsby": "^2.9.4",
    "gatsby-image": "^2.1.4",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.3",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.1.5",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-source-filesystem": "^2.0.39",
    "gatsby-transformer-json": "^2.1.11",
    "gatsby-transformer-sharp": "^2.1.21",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-fairy-gates": "^0.16.19"
  },
  "devDependencies": {
    "dotenv": "^8.0.0",
    "gh-pages": "^2.0.1",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "medea": "file:../medea",
    "prettier": "^1.18.2"
  },
  "keywords": [
    "gatsby",
    "seasonal anime",
    "anime ratings",
    "myanimelist"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,json,scss,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty"
    }
  },
  "scripts": {
    "clear-cache": "rmdir /s /q public && rmdir /s /q .cache",
    "build": "npm run clear-cache && gatsby build",
    "develop": "npm run clear-cache && gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "pretty": "lint-staged",
    "cli": "node ./tools",
    "deploy": "yarn build --prefix-paths && gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bakuzan/hierophant-green"
  },
  "bugs": {
    "url": "https://github.com/bakuzan/hierophant-green/issues"
  }
}
